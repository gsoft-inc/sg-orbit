{
    "name": "@orbit-ui/react-components",
    "version": "1.0.3",
    "description": "Sharegate Orbit UI React components",
    "repository": "https://github.com/gsoft-inc/sg-brand.git",
    "author": "Groupe Sharegate inc.",
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "start": "yarn watch",
        "prebuild": "yarn clean",
        "build": "run-p build:*",
        "build:this": "babel src -d dist --config-file ./babel.config.js",
        "build:components": "lerna exec --scope @orbit-ui/react-* --ignore @orbit-ui/react-components --parallel -- babel src -d dist --config-file ../../babel.config.js --copy-files",
        "prewatch": "yarn clean",
        "watch": "run-p \"build:* --watch\"",
        "clean": "run-p clean:*",
        "clean:this": "rimraf dist",
        "clean:components": "lerna exec --scope @orbit-ui/react-* --ignore @orbit-ui/react-components --parallel -- rimraf dist",
        "prepublishOnly": "cross-env NODE_ENV=production && yarn build"
    },
    "peerDependencies": {
        "@orbit-ui/css-normalize": "*",
        "@orbit-ui/icons": "*",
        "@orbit-ui/semantic-ui-theme": "*",
        "@orbit-ui/tachyons": "*",
        "react": "*",
        "react-dom": "*",
        "semantic-ui-react": "*",
        "styled-jsx": "*"
    },
    "dependencies": {
        "@orbit-ui/react-components-shared": "1.0.0",
        "@orbit-ui/react-date-picker": "0.1.0",
        "@orbit-ui/react-multi-select": "1.0.2",
        "@orbit-ui/react-popup": "1.0.0",
        "@orbit-ui/react-search-input": "1.0.1"
    },
    "gitHead": "12c3852000398fe0e93b8ebc2548bcb026478bfa"
}
