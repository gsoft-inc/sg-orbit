        }
    },
    "devDependencies": {
        "@swc/core": "1.3.85",
        "@swc/helpers": "0.5.2",
        "@swc/jest": "0.2.29",
        "@types/eslint": "8.44.2",
        "@types/estree": "1.0.1",
        "@types/jest": "29.5.5",
        "@types/node": "20.6.2",
        "@workleap/swc-configs": "2.1.2",
        "@workleap/tsup-configs": "3.0.1",
        "@workleap/typescript-configs": "3.0.2",
        "eslint": "8.49.0",
        "jest": "29.7.0",
        "ts-node": "10.9.1",
        "tsup": "7.2.0",
        "typescript": "5.2.2"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
